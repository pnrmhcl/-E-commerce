
<div class="row">
    <div class="col-sm-12 p-0">
        <div class="main-header" style="margin-top: 0px;">
            <h4>Kategori Ekle</h4>
        </div>
    </div>
</div>


<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <h5 class="card-header-text">Ekle</h5>
            </div>


            <div class="card-block">
                <form asp-action="Add" method="post" enctype="multipart/form-data">
                    <div class="form-group">
                        <label for="1" class="form-control-label">Kategori Adı</label>
                        <input type="text" class="form-control" id="1" aria-describedby="emailHelp"  name="categoryName">
                    </div>
                    <button type="submit" class="btn btn-success waves-effect waves-light m-r-30">Ekle</button>
                </form>
            </div>
        </div>
    </div>

</div>